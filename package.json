{
  "name": "backend-digitro",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --poll --transpile-only --ignore-watch node_modules --respawn ./src/server.ts",
    "test": "jest",
    "start": "node dist/server.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm/cli.js -d ./src/data-source.ts",
    "migration:create": "typeorm migration:create",
    "migration:run": "npm run typeorm migration:run",
    "migration:down": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "aws-sdk": "^2.1359.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dinero.js": "^1.9.1",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.6.0",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "tscpaths": "^0.0.9",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.3.14",
    "uuid": "^8.3.2",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/dinero.js": "^1.9.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
